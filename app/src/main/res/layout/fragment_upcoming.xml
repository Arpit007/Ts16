<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context="com.nitkkr.gawds.ts16.upcomingFragment">
    <TextView android:id="@+id/NoUpcoming"
        android:text="@string/NoUpcoming" android:visibility="invisible"
        android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:layout_gravity="center"/>
    <ListView android:id="@+id/upcomingList" android:scrollbars="none"
              android:divider="#00ffffff" android:dividerHeight="20dp"
              android:paddingTop="30dp" android:paddingLeft="10dp" android:paddingRight="10dp"
              android:layout_width="match_parent" android:layout_height="wrap_content"/>

</FrameLayout>
